# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
insert_final_newline = true
max_line_length = 120
trim_trailing_whitespace = true

[*.java]
ij_continuation_indent_size = 8

[*.md]
max_line_length = off
trim_trailing_whitespace = false
